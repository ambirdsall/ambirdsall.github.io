{
  "name": "blag",
  "private": true,
  "description": "A blog.",
  "version": "0.1.0",
  "author": "Alex Birdsall <ambirdsall@gmail.com>",
  "scripts": {
    "build":  "gatsby build",
    "watch":  "ls gatsby-config.js | entr -r gatsby develop",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md}\"",
    "check":  "prettier --check \"src/**/*.{js,jsx,json,md}\"",
    "start":  "gatsby develop",
    "serve":  "gatsby serve",
    "test":   "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "gatsby build && gh-pages -d public -b master"
  },
  "dependencies": {
    "d3": "^5.14.2",
    "gatsby": "^3",
    "gatsby-image": "^2.2.31",
    "gatsby-plugin-manifest": "^3",
    "gatsby-plugin-offline": "^4",
    "gatsby-plugin-react-helmet": "^4",
    "gatsby-plugin-sharp": "^3",
    "gatsby-remark-autolink-headers": "^3",
    "gatsby-remark-footnotes": "0.0.7",
    "gatsby-remark-images": "^4",
    "gatsby-remark-prismjs": "^4",
    "gatsby-source-filesystem": "^3",
    "gatsby-transformer-remark": "^3",
    "gatsby-transformer-sharp": "^3",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.8.0"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "prettier": "^1.19.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ambirdsall/ambirdsall.github.io"
  }
}
